{
    "name": "design-system-wordpress-gutenberg",
    "version": "1.2.0",
    "description": "Gutenberg Blocks integration for the California Design System",
    "main": "index.js",
    "scripts": {
      "lint": "semistandard",
      "test": "echo \"Error: no test specified\" && exit 1",
      "start": "wp-scripts start",
      "build": "wp-scripts build",
      "new-block": "cd blocks/ && npx @wordpress/create-block %npm_config_name% --template ../template/index.js && cd ../",
      "postnew-block": "npm run build",
      "prebuild": "gulp build-blocks",
      "build": " gulp build",
      "install": "npm i -g npm-check-updates",
      "postinstall": "npm config set gulp-cli --global && npm rebuild node-sass"
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/cagov/design-system-wordpress-gutenberg.git"
    },
    "keywords": ["California Design System"],
    "author": "Office of Digital Innovation",
    "license": "MIT",
    "bugs": {
      "url": "https://github.com/cagov/design-system-wordpress-gutenberg/issues"
    },
    "homepage": "https://github.com/cagov/design-system-wordpress-gutenberg#README",
    "devDependencies": {
      "@wordpress/scripts": "^16.1.5",
      "semistandard": "^16.0.0",
      "@open-wc/eslint-config": "^4.3.0",
      "eslint": "^7.32.0",
      "eslint-config-prettier": "^8.3.0",
      "prettier": "^2.4.1",
      "del": "^6.0.0",
      "fancy-log": "^2.0.0",
      "gulp": "^4.0.2",
      "gulp-cli": "^2.3.0",
      "gulp-concat": "^2.6.1",
      "gulp-html-beautify": "^1.0.1",
      "gulp-line-ending-corrector": "^1.0.3",
      "gulp-run-command": "^0.0.10",
      "gulp-sass": "^5.1.0",
      "gulp-shell": "^0.8.0",
      "gulp-tap": "^2.0.0",
      "gulp-uglify-es": "^3.0.0",
      "gulp-watch": "^5.0.1",
      "node-sass": "^7.0.1",
      "npm-check-updates": "^12.5.9",
      "yargs": "^17.4.1"
    },
    "prettier": {
        "singleQuote": true,
        "trailingComma": "all"
    },
    "workspaces": [
        "./blocks/*"
    ],
    "dependencies": {
      "@cagov/ds-accordion": "^2.0.0",
      "@cagov/ds-base-css": "^1.0.15",
      "@cagov/ds-feature-card": "^1.0.6",
      "@cagov/ds-feedback": "^1.0.12",
      "@cagov/ds-highlight-section": "^1.0.1",
      "@cagov/ds-link-grid": "^2.1.0",
      "@cagov/ds-minus": "^1.0.1",
      "@cagov/ds-pagination": "^2.0.0",
      "@cagov/ds-page-alert": "^2.0.0",
      "@cagov/ds-page-navigation": "^2.1.0",
      "@cagov/ds-pdf-icon": "^1.0.1",
      "@cagov/ds-plus": "^1.0.1",
      "@cagov/ds-regulatory-outline": "^1.0.1",
      "@cagov/ds-step-list": "^2.0.1",
      "@wordpress/block-editor": "^6.1.3",
      "@wordpress/blocks": "^9.1.3",
      "@wordpress/components": "^14.1.2",
      "@wordpress/compose": "^5.0.2",
      "@wordpress/data": "^6.1.0",
      "@wordpress/date": "^4.1.1",
      "@wordpress/element": "^3.1.1",
      "@wordpress/i18n": "^4.1.1",
      "lodash": "^4.17.21",
      "moment": "^2.29.1"
